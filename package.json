{
  "name": "yuki-no",
  "description": "GitHub Action for tracking upstream documentation changes",
  "repository": {
    "url": "https://github.com/Gumball12/yuki-no.git",
    "type": "git"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "tsx src/index.ts",
    "lint": "prettier --check --write --parser typescript \"{src,test}/**/*.ts\"",
    "test": "yarn lint && vitest --coverage --run"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@types/shelljs": "^0.8.15",
    "colors": "^1.4.0",
    "queue": "^7.0.0",
    "rss-parser": "^3.13.0",
    "shelljs": "^0.8.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "^2.1.8",
    "prettier": "^3.4.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
